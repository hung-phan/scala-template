  "scripts": {
    "build": "./scripts/prod.sh",
    "clean": "rimraf build",
    "dev": "./scripts/dev.sh",
    "flow-status": "flow status; test $? -eq 0 -o $? -eq 2",
    "flow-stop": "flow stop",
    "flow-watch": "yarn run flow:status && chokidar './src/**/*.js' -c 'yarn run flow-status'",
    "test": "env-cmd --no-override ./scripts/env/test jest"
  }
